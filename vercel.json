{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}